{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "src/**/*",
    ".vitepress/theme/**/*",
    "types/**/*",
  ],
  "exclude": [
    "/node_modules",
    "/docs",
    "/.cache",
  ],
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "types": [
      "vite/client",
    ], // /// <reference types="vite/client" />
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "img/*": [
        "src/assets/images/*"
      ]
    }
  }
}
